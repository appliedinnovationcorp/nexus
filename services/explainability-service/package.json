{
  "name": "@nexus/explainability-service",
  "version": "1.0.0",
  "description": "AI explainability and audit trail service with model interpretability and decision tracking",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --exec ts-node src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "docker:build": "docker build -t nexus/explainability-service .",
    "docker:run": "docker run -p 8004:8004 nexus/explainability-service"
  },
  "dependencies": {
    "@nexus/domain-models": "workspace:*",
    "@nexus/event-bus": "workspace:*",
    "@nexus/observability": "workspace:*",
    "@nexus/security": "workspace:*",
    "fastify": "^4.28.1",
    "@fastify/cors": "^8.0.0",
    "@fastify/helmet": "^11.0.0",
    "shap": "^0.1.0",
    "lime-js": "^0.2.1",
    "@tensorflow/tfjs-node": "^4.22.0",
    "mongodb": "^6.10.0",
    "elasticsearch": "^16.7.3",
    "uuid": "^10.0.0",
    "winston": "^3.15.0",
    "prometheus-client": "^15.1.3",
    "zod": "^3.23.8",
    "crypto": "^1.0.1"
  },
  "devDependencies": {
    "@nexus/typescript-config": "workspace:*",
    "@types/node": "^22.10.1",
    "@types/uuid": "^10.0.0",
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "nodemon": "^3.1.7",
    "ts-node": "^10.9.2",
    "typescript": "5.8.2",
    "eslint": "^8.57.1",
    "supertest": "^7.0.0"
  },
  "keywords": [
    "explainability",
    "interpretability",
    "audit-trail",
    "ai-governance",
    "model-transparency",
    "compliance",
    "shap",
    "lime"
  ]
}
